<html>
<head>
<title>XGrid</title>
<link type="text/css" href="<%= XgridConfig.baseurl %>/css/xgrid.css" rel="stylesheet"/>
</head>
<body>
<div id="page">
<div class="header">
<h1 id="logo"><a href="<%= XgridConfig.baseurl+"/" %>">
<img src="../images/logo.png" alt="XGrid"/>
</a></h1>
<ul class="nav">
<li><a href="<%= XgridConfig.baseurl+"/admin" %>">Administration</a></li><li><a href="<%= XgridConfig.baseurl+"/admin/ec2" %>">EC2</a></li>
<% XgridConfig.dashboard.each do|name,route|%>
<li>
<a href="<%= XgridConfig.baseurl+route %>"><%= name %></a>
</li>
<% end %>
</ul>
<hr/>
</div>
<div class="content">
<%= yield %>
</div>
<div class="footer">
</div>
</body>
</div>
</html>
