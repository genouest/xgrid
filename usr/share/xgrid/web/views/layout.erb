<html>
<head>
<title>XGrid</title>
<script type="text/javascript" src="<%= XgridConfig.baseurl %>/js/jquery-1.7.1.min.js"></script>
<script type="text/javascript" src="<%= XgridConfig.baseurl %>/js/jquery-ui-1.8.18.custom.min.js"></script>
<link type="text/css" href="<%= XgridConfig.baseurl %>/css/xgrid.css" rel="stylesheet"/>
<link type="text/css" href="<%= XgridConfig.baseurl %>/css/smoothness/jquery-ui-1.8.18.custom.css" rel="stylesheet"/>
<script>
 $(document).ready(function() {
 });
</script>
</head>
<body>
<div id="page">
<div class="header">
<img id="logo"  src="<%= XgridConfig.baseurl %>/images/logo.png" alt="XGrid"/>
<ul class="nav">
<li><a href="<%= XgridConfig.baseurl+"/admin" %>">Administration</a></li><li><a href="<%= XgridConfig.baseurl+"/admin/ec2" %>">EC2</a></li>
<% XgridConfig.dashboard.each do|name,route|%>
<li>
<a href="<%= XgridConfig.baseurl+route %>"><%= name %></a>
</li>
<% end %>
</ul>
<hr/>
</div>
<div class="content">
<%= yield %>
</div>
<div class="footer">
</div>
</div>
</body>
</html>
